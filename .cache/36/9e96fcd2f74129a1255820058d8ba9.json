as Neue', 'Franklin Gothic Condensed', 'Arial', sans-serif;\n$bodyfont-family: 'Cormorant Garamond', 'Lora', serif;\n\n//TYPOGRAPHIC SCALE_______________________________________________________\nh3, .small_text { \n  margin: .5em 0;\n  color: $color-light-accent; \n}  \n\nh2, h3{\n  font-family: $bodyfont-family;\n  text-transform: uppercase;\n  color: $color-light-accent; \n}\n\nh3, .small_text{\n  font-weight: 100;\n  letter-spacing: .2em; \n}\n// _____\n\nh2 {\n  font-size: 6vw;\n  margin-bottom: 0;\n  margin-top: 0;\n  letter-spacing: .05em;\n}\n\nh3 {\n  margin-top: 0;\n  font-size: 2vw;\n  color: $color-light-accent;\n  font-weight: 700;\n}\n\n.small_text{\n  font-size: 2vw;\n  margin-bottom: 0;\n  font-family: $headingfont-family; \n  text-transform: uppercase;\n}\n\n//HERO PAGE TYPE &STYLING_________________________________________________\n\n#hero {\npadding: $section-padding;\nposition: relative;\n\n  .full-height {\n    height: 100vh;\n  }\n}\n\n.container{\n  padding: 0 50px;\n  max-width: 1400px;\n  margin: 0 auto;\n    display: grid;\n    grid-gap: 0;\n    place-items: center;\n    grid-template-columns: repeat(7, 1fr);\n    grid-template-areas:\n      \"fairy head head head head head .\"\n      \". id_invite id_invite line anim_invite anim_invite nessy_container\";\n  \n  .fairy{\n    grid-area: fairy;\n    width: 15vw;\n    height: auto;\n    visibility: visible;\n    z-index: 100;\n    margin-left: -4.5vw;\n    top: 10vh;\n    position: absolute;\n  } \n\n  .head {\n    grid-area: head;\n    margin-bottom: 3em;\n    .title{\n      width: 100%;\n      height: auto;\n      z-index:100;\n    }\n  }\n\n  .id_invite {\n    grid-area: id_invite;\n  }\n\n  .line{\n    grid-area: line;\n    height: 100%;\n    border-right: 1px solid white;\n  }\n\n  .anim_invite {\n    grid-area: anim_invite;\n  }\n\n  .nessy_container{\n    grid-area: nessy_container;\n    .nessy{\n      width: 30vw;\n      bottom: 0;\n      right: -15vw;\n      position: absolute;\n    }\n  }\n}\n\n@media (max-width: $breakpoint-small){\n  #hero{\n    padding-top: 4vh;\n  }\n  \n  .container{\n    display: block;\n    padding-top: 0;\n    \n// type_______\n  h2, h3, .small_text, .anim_invite, .id_invite{\n    text-align: center;\n  }\n\n  .small_text{\n    font-size: 1.3rem;\n  }\n\n  .anim_invite{\n    margin-top: 2em;\n  }\n  \n \n    h2{\n      font-size: 4rem;\n    }\n\n    h3{\n      font-size: 1.4rem;\n    }\n\n    .save_date{\n      font-size: 8vw;\n    }\n    \n    .web_soon_text{\n      font-size: 4vw;\n    }\n\n    .line{\n      grid-area: line;\n     width: 48vw;\n     margin:0 auto;\n      padding-bottom: 2vh;\n      border-right: 0;\n      border-bottom: 1px solid white;\n    }\n\n// characters___\n    .nessy, .fairy{\n      visibility: hidden;\n    }\n  }\n}  \n\n// Base (COLOR BACKGROUND)__________________________\nhtml {\n  box-sizing: border-box;\n}\n  \n*, \n*:before, \n*:after {\n  box-sizing: inherit;\n}\n  \nbody{\n  background-image: url(img/background.jpg);\n  background-repeat: none;\n  overflow: hidden;\n}"},"lineCount":null}},"error":null,"hash":"46de6a0e092d62cdbce2ba0ba6fa6566","cacheData":{"env":{}}}