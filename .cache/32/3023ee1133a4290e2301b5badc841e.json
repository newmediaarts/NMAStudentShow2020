:0;\n  font-size: 3.2vw;\n  font-family: $bodyfont-family;\n  color:  $color-light-accent;\n  text-transform: none;\n  font-weight: 300;\n  margin-bottom: 4rem;\n  text-align: center;\n  margin-top: -.6em;\n  margin-bottom: 7vh;\n  letter-spacing: .03em; \n}\n\nh2 {\n  font-size: 6vw;\n  margin-bottom: 0;\n  margin-top: 0;\n  letter-spacing: .05em;\n}\n\nh3 {\n  margin-top: 0;\n  font-size: 2vw;\n  color: $color-light-accent;\n  font-weight: 700;\n}\n\n.small_text{\n  font-size: 2vw;\n  margin-bottom: 0;\n}\n\n// \"save the date\" format\n.save_date{\nmargin-top: 6vh;\npadding-bottom: 0;\nfont-style: italic;\ntext-transform: uppercase;\ntext-align: center;\nfont-size: 5vw;\n}\n\n.web_soon_text{\npadding-top: 0;\ncolor: $color-background-lightgray;\ntext-align: center;\n} \n\n//HERO AND COMING SOON PAGE TYPE &STYLING_________________________________________________\n\n#hero {\npadding: $section-padding;\n  .full-height {\n    height: 100vh;\n  }\n}\n\n.container{\n  padding: 0 50px;\n  max-width: 1600px;\n  margin: 0 auto;\n  display: grid;\n  grid-gap: 0;\n  place-items: center;\n  grid-template-columns: repeat(7, 1fr);\n  grid-template-areas:\n    \". head head head head head .\"\n    \". id_invite id_invite line anim_invite anim_invite .\"\n    \". save_date_web save_date_web save_date_web save_date_web save_date_web .\";\n   \n  .head {\n    grid-area: head;\n  }\n\n  .id_invite {\n    grid-area: id_invite;\n  }\n\n  .line{\n    grid-area: line;\n    height: 100%;\n    border-right: 1px solid white;\n  }\n\n  .anim_invite {\n    grid-area: anim_invite;\n  }\n\n  .save_date_web {\n    grid-area: save_date_web;\n  }\n}\n\n@media (max-width: $breakpoint-small){\n  #hero{\n    padding-top: 4rem;\n  }\n  \n  .container{\n    display: block;\n    padding-top: 0;\n    \n\n  h2, h3, .small_text, .anim_invite, .id_invite{\n    text-align: center;\n  }\n\n  h1{\n    font-size: 15vw;\n    margin-bottom: 0;\n  }\n\n  .subhead{\n    font-size: 4vw;\n  }\n\n  .small_text{\n    font-size: 1.3rem;\n  }\n\n  .anim_invite{\n    margin-top: 2em;\n  }\n  \n  // type_______\n    h2{\n      font-size: 4rem;\n    }\n\n    h3{\n      font-size: 1.4rem;\n    }\n\n    .save_date{\n      font-size: 8vw;\n    }\n    \n    .web_soon_text{\n      font-size: 4vw;\n    }\n\n    .line{\n      grid-area: line;\n      height: 100%;\n      padding-bottom: 2vh;\n      border-right: 0;\n      border-bottom: 1px solid white;\n    }\n  }\n}  \n\n// Base (COLOR BACKGROUND)__________________________\nhtml {\n  box-sizing: border-box;\n}\n  \n*, \n*:before, \n*:after {\n  box-sizing: inherit;\n}\n  \nbody{\n  background-image: url(img/background.jpg);\n}\n"},"lineCount":null}},"error":null,"hash":"d99057861c0fd087db14d02915a407ef","cacheData":{"env":{}}}